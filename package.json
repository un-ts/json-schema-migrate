{
  "name": "@unts/json-schema-migrate",
  "version": "2.0.0",
  "type": "module",
  "description": "Migrate JSON-Schema to draft-06",
  "repository": "https://github.com/un-ts/json-schema-migrate.git",
  "homepage": "https://github.com/un-ts/json-schema-migrate",
  "author": "Evgeny Poberezkin",
  "maintainers": [
    "JounQin <admin@1stg.me> (https://www.1stG.me)"
  ],
  "license": "MIT",
  "packageManager": "yarn@4.8.1",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "module-sync": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./index.d.cts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "types": "lib/index.d.ts",
  "files": [
    "index.d.cts",
    "lib",
    "!lib/*.tsbuildinfo"
  ],
  "keywords": [
    "JSON-Schema",
    "migrate",
    "draft-06"
  ],
  "scripts": {
    "build": "run-p 'build:*'",
    "build:r": "r -f cjs",
    "build:tsc": "tsc -b src",
    "format": "prettier --write .",
    "lint": "run-p 'lint:*'",
    "lint:es": "eslint . --cache",
    "lint:tsc": "tsc --noEmit",
    "prepare": "patch-package && simple-git-hooks && yarn-berry-deduplicate || exit 0",
    "release": "clean-pkg-json && changeset publish",
    "test": "vitest run",
    "version": "changeset version && yarn --no-immutable"
  },
  "dependencies": {
    "@pkgr/core": "^0.2.0",
    "ajv": "^8.17.1",
    "ajv-draft-04": "^1.0.0"
  },
  "devDependencies": {
    "@1stg/common-config": "^11.1.1",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@commitlint/cli": "^19.8.0",
    "@pkgr/rollup": "^6.0.1",
    "@types/node": "^22.13.17",
    "@unts/patch-package": "^8.1.1",
    "@vitest/coverage-istanbul": "3.1.1",
    "clean-pkg-json": "^1.2.1",
    "eslint": "^9.23.0",
    "lint-staged": "^15.5.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "simple-git-hooks": "^2.12.1",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1",
    "yarn-berry-deduplicate": "^6.1.1"
  },
  "resolutions": {
    "prettier": "^3.5.3"
  }
}
